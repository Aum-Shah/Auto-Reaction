{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "BOT_TOKEN": "Telegram Bot Token",
      "BOT_USERNAME": "Username Of Bot (Without @)",
      "EMOJI_LIST": "Emoji List (Without , )",
      "RESTRICTED_CHATS": "Restricted Chats Id (-100294948593,3848582834)"
    }
  }
  